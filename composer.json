{
  "name": "mizmoz/container",
  "description": "Mizmoz DI Container",
  "type": "library",
  "version": "2.0.1",
  "license": "MIT",
  "authors": [
    {
      "name": "Ian",
      "email": "chad@mizmoz.com"
    }
